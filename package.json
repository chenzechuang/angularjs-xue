{
  "name": "angularjs-xue",
  "version": "1.0.0",
  "description": "components for angularjs by Gosuncn FE team",
  "homepage": "https://github.com/zhangxuelian/angularjs-xue",
  "keywords": [
    "angularjs",
    "ui"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangxuelian/angularjs-xue"
  },
  "author": "https://github.com/zhangxuelian/angularjs-xue/graphs/contributors",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "angular": "^1.2.32",
    "angular-mocks": "^1.2.32",
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-eslint": "^22.0.0",
    "grunt-html2js": "^0.7.1",
    "grunt-karma": "^3.0.2",
    "grunt-sass": "^3.1.0",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "^3.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "load-grunt-tasks": "^5.1.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 5
    },
    "env": {
      "browser": true,
      "jasmine": true,
      "node": true
    },
    "globals": {
      "angular": true,
      "inject": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "comma-dangle": 2,
      "no-unused-vars": 1,
      "no-empty": 1,
      "no-extra-parens": 1,
      "accessor-pairs": 2,
      "block-scoped-var": 2,
      "complexity": [
        1,
        5
      ],
      "curly": 1,
      "dot-notation": [
        1,
        {
          "allowKeywords": false
        }
      ],
      "eqeqeq": 1,
      "guard-for-in": 1,
      "semi": 1,
      "no-alert": 2,
      "no-caller": 2,
      "no-div-regex": 2,
      "no-else-return": 1,
      "no-eq-null": 2,
      "no-eval": 2,
      "no-extend-native": 2,
      "no-extra-bind": 1,
      "no-floating-decimal": 2,
      "no-implied-eval": 2,
      "no-invalid-this": 2,
      "no-lone-blocks": 2,
      "no-loop-func": 2,
      "no-multi-spaces": 2,
      "no-new-func": 2,
      "no-new-wrappers": 2,
      "no-new": 2,
      "no-octal-escape": 2,
      "no-proto": 2,
      "no-script-url": 2,
      "no-self-compare": 2,
      "no-sequences": 2,
      "no-throw-literal": 2,
      "no-unused-expressions": 2,
      "no-useless-call": 2,
      "no-useless-concat": 2,
      "no-void": 2,
      "no-with": 2,
      "radix": 2,
      "wrap-iife": 2,
      "no-iterator": 2,
      "yoda": 2
    }
  }
}